// render results partial
$('#results').html('<%= escape_javascript(render :partial => "shared/results_table_form", :locals => {:games => @games }) %>');

// check selected results
$(function(){
	var result;
	var results_list = get_results_list();
	for(i=0;i<results_list.length;i++){
		result=document.getElementById(results_list[i]);
		if (result != null && result != undefined) {
			document.getElementById(results_list[i]).checked = true;
		}
	}
});



